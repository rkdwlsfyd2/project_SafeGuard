{
    "family": "safeguard-task",
    "executionRoleArn": "arn:aws:iam::274799453401:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "safeguard-frontend",
            "image": "274799453401.dkr.ecr.ap-northeast-2.amazonaws.com/safeguard-frontend:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "name": "safeguard-frontend-80-tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/safeguard-task",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        },
        {
            "name": "safeguard-ai-yolo",
            "image": "274799453401.dkr.ecr.ap-northeast-2.amazonaws.com/safeguard-ai-yolo:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp",
                    "name": "safeguard-ai-yolo-5000-tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/safeguard-task",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": [],
            "memory": 6144,
            "memoryReservation": 2048
        },
        {
            "name": "safeguard-ai-stt",
            "image": "274799453401.dkr.ecr.ap-northeast-2.amazonaws.com/safeguard-ai-stt:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp",
                    "name": "safeguard-ai-stt-8000-tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/safeguard-task",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": [],
            "memory": 4096,
            "memoryReservation": 1024
        },
        {
            "name": "safeguard-backend",
            "image": "274799453401.dkr.ecr.ap-northeast-2.amazonaws.com/safeguard-backend:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "name": "safeguard-backend-8080-tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "SPRING_DATASOURCE_USERNAME",
                    "value": "postgres"
                },
                {
                    "name": "SPRING_DATASOURCE_URL",
                    "value": "jdbc:postgresql://safeguard-db.c722smg6e53a.ap-northeast-2.rds.amazonaws.com:5432/postgres"
                },
                {
                    "name": "SPRING_DATASOURCE_PASSWORD",
                    "value": "postgres"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/safeguard-task",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": [],
            "memory": 2048,
            "memoryReservation": 1024
        },
        {
            "name": "milvus-minio",
            "image": "minio/minio:RELEASE.2023-03-20T20-16-18Z",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 9000,
                    "hostPort": 9000,
                    "protocol": "tcp",
                    "name": "minio-9000-tcp"
                },
                {
                    "containerPort": 9001,
                    "hostPort": 9001,
                    "protocol": "tcp",
                    "name": "minio-9001-tcp"
                }
            ],
            "essential": true,
            "command": [
                "minio",
                "server",
                "/data",
                "--console-address",
                ":9001"
            ],
            "environment": [
                {
                    "name": "MINIO_ACCESS_KEY",
                    "value": "minioadmin"
                },
                {
                    "name": "MINIO_SECRET_KEY",
                    "value": "minioadmin"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/safeguard-task",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        },
        {
            "name": "milvus-etcd",
            "image": "quay.io/coreos/etcd:v3.5.5",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 2379,
                    "hostPort": 2379,
                    "protocol": "tcp",
                    "name": "etcd-2379-tcp"
                }
            ],
            "essential": true,
            "command": [
                "etcd",
                "-advertise-client-urls",
                "http://127.0.0.1:2379",
                "-listen-client-urls",
                "http://0.0.0.0:2379",
                "--data-dir",
                "/etcd"
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/safeguard-task",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        },
        {
            "name": "milvus-standalone",
            "image": "milvusdb/milvus:v2.3.4",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 19530,
                    "hostPort": 19530,
                    "protocol": "tcp",
                    "name": "milvus-19530-tcp"
                },
                {
                    "containerPort": 9091,
                    "hostPort": 9091,
                    "protocol": "tcp",
                    "name": "milvus-9091-tcp"
                }
            ],
            "essential": true,
            "command": [
                "milvus",
                "run",
                "standalone"
            ],
            "environment": [
                {
                    "name": "ETCD_ENDPOINTS",
                    "value": "127.0.0.1:2379"
                },
                {
                    "name": "MINIO_ADDRESS",
                    "value": "127.0.0.1:9000"
                }
            ],
            "dependsOn": [
                {
                    "containerName": "milvus-etcd",
                    "condition": "START"
                },
                {
                    "containerName": "milvus-minio",
                    "condition": "START"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/safeguard-task",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "memory": 6144,
            "memoryReservation": 2048
        },
        {
            "name": "safeguard-ai-rag",
            "image": "274799453401.dkr.ecr.ap-northeast-2.amazonaws.com/safeguard-ai-rag:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8001,
                    "hostPort": 8001,
                    "protocol": "tcp",
                    "name": "ai-rag-8001-tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "MILVUS_HOST",
                    "value": "127.0.0.1"
                },
                {
                    "name": "MILVUS_PORT",
                    "value": "19530"
                }
            ],
            "dependsOn": [
                {
                    "containerName": "milvus-standalone",
                    "condition": "START"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/safeguard-task",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "4096",
    "memory": "16384",
    "ephemeralStorage": {
        "sizeInGiB": 100
    },
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}